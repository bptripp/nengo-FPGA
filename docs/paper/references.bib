file = {:Users/celiasmi/Google Drive/Chris Documents/Mendeley Papers/2002/Koulakov et al. - 2002 - Model for a robust neural integrator - Nature neuroscience.pdf:pdf},
isbn = {1097-6256},
journal = {Nature neuroscience},
keywords = {Computer Simulation; Models,N-Methyl-D-Aspartate/physiology; Reproducibility,Neurological; Neural Networks (Computer); Neurons},
month = aug,
number = {8},
