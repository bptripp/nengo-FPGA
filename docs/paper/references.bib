pages = {283--328},
pmid = {16378516},
publisher = {MIT Press  238 Main St., Suite 500, Cambridge, MA 02142-1046 USA journals-info@mit.edu},
title = {{Making working memory work: a computational model of learning in the prefrontal cortex and basal ganglia.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16378516 http://www.mitpressjournals.org/doi/abs/10.1162/089976606775093909},
