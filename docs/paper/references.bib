journal = {Journal of physiology, Paris},
keywords = {Action Potentials/physiology; Computational Biolog,Neurological; Neurons/physiology; Parkinson Disea},
number = {1-3},
pages = {56--63},
title = {{New insights offered by a computational model of deep brain stimulation}},
volume = {101},
year = {2007}
}
