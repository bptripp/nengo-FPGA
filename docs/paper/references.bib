month = jun,
number = {7045},
pages = {1102--7},
pmid = {15973409},
shorttitle = {Nature},
title = {{Invariant visual representation by single neurons in the human brain.}},
url = {http://dx.doi.org/10.1038/nature03687},
volume = {435},
year = {2005}
}
