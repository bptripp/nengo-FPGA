keywords = {Animals,Axons,Axons: physiology,Computer Simulation,Computing Methodologies,Hippocampus,Hippocampus: anatomy \& histology,Hippocampus: physiology,Learning,Learning: physiology,Models,Neurological,Neurons,Neurons: classification,Neurons: cytology,Neurons: physiology,Probability,Psychological,Rats,Robotics,Space Perception,Space Perception: physiology},
