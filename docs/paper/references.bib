keywords = {Cognition,Cognition: physiology,Creativeness,Generalization (Psychology),Generalization (Psychology): physiology,Humans,Language,Long-Term Potentiation,Long-Term Potentiation: physiology,Memory,Models, Theoretical,Neural Networks (Computer),Time Factors},
