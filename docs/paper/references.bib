journal = {The Neuroscientist : a review journal bringing neurobiology, neurology and psychiatry},
keywords = {Animals,Cerebral Cortex,Cerebral Cortex: anatomy \& histology,Cerebral Cortex: physiology,Humans,Memory,Memory Disorders,Memory Disorders: physiopathology,Models,Nerve Net,Nerve Net: anatomy \& histology,Nerve Net: physiology,Neural Inhibition,Neural Inhibition: physiology,Neural Pathways,Neural Pathways: anatomy \& histology,Neural Pathways: physiology,Neurological,Neurons,Neurons: physiology,Short-Term,Short-Term: physiology,Space Perception,Space Perception: physiology},
month = dec,
number = {6},
pages = {553--65},
pmid = {15534040},
title = {{A neural circuit basis for spatial working memory}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15534040},
