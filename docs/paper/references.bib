@article{Cohen2004,
abstract = {Neurophysiologists have shown repeatedly that neural activity in different brain structures can be correlated with specific perceptual and cognitive functions, but the causal efficacy of the observed activity has generally been a matter of conjecture. By contrast, electrical microstimulation, which allows the experimenter to manipulate the activity of small groups of neurons with spatial and temporal precision, can now be used to demonstrate causal links between neural activity and specific cognitive functions. Here, we review this growing literature, including applications to the study of attention, visual and somatosensory perception, 'read-out' mechanisms for interpreting sensory maps, and contextual effects on perception. We also discuss potential applications of microstimulation to studies of higher cognitive functions such as decision-making and subjective experience.},
author = {Cohen, Marlene R and Newsome, William T},
doi = {10.1016/j.conb.2004.03.016},
file = {:Users/celiasmi/Google Drive/Chris Documents/Mendeley Papers/2004/Cohen, Newsome - 2004 - What electrical microstimulation has revealed about the neural basis of cognition. - Current opinion in neurobio.pdf:pdf},
issn = {0959-4388},
journal = {Current opinion in neurobiology},
keywords = {Action Potentials,Action Potentials: physiology,Animals,Attention,Attention: physiology,Brain,Brain: cytology,Brain: physiology,Cognition,Cognition: physiology,Electric Stimulation,Electric Stimulation: methods,Humans,Motion Perception,Motion Perception: physiology,Neural Pathways,Neural Pathways: cytology,Neural Pathways: physiology,Neurophysiology,Neurophysiology: methods,Neurophysiology: trends},
