journal = {Nature neuroscience},
keywords = {Animals,Dendrites,Dendrites: physiology,Dendrites: ultrastructure,Excitatory Postsynaptic Potentials,Excitatory Postsynaptic Potentials: physiology,Neocortex,Neocortex: physiology,Nerve Net,Nerve Net: physiology,Nerve Net: ultrastructure,Pyramidal Cells,Pyramidal Cells: physiology,Pyramidal Cells: ultrastructure,Rats,Wistar},
