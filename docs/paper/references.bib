@article{Schiller2000,
abstract = {Basal dendrites are a major target for synaptic inputs innervating cortical pyramidal neurons. At present little is known about signal processing in these fine dendrites. Here we show that coactivation of clustered neighbouring basal inputs initiated local dendritic spikes, which resulted in a 5.9 +/- 1.5 mV (peak) and 64.4 +/- 19.8 ms (half-width) cable-filtered voltage change at the soma that amplified the somatic voltage response by 226 +/- 46\%. These spikes were accompanied by large calcium transients restricted to the activated dendritic segment. In contrast to conventional sodium or calcium spikes, these spikes were mediated mostly by NMDA (N-methyl-D-aspartate) receptor channels, which contributed at least 80\% of the total charge. The ionic mechanism of these NMDA spikes may allow 'dynamic spike-initiation zones', set by the spatial distribution of glutamate pre-bound to NMDA receptors, which in turn would depend on recent and ongoing activity in the cortical network. In addition, NMDA spikes may serve as a powerful mechanism for modification of the cortical network by inducing long-term strengthening of co-activated neighbouring inputs.},
author = {Schiller, J and Major, G and Koester, H J and Schiller, Y},
doi = {10.1038/35005094},
issn = {0028-0836},
journal = {Nature},
keywords = {2-Amino-5-phosphonovalerate,2-Amino-5-phosphonovalerate: pharmacology,Animals,Cadmium,Cadmium: pharmacology,Calcium,Calcium Channel Blockers,Calcium Channel Blockers: pharmacology,Calcium: metabolism,Cerebral Cortex,Cerebral Cortex: cytology,Cerebral Cortex: physiology,Computer Simulation,Dendrites,Dendrites: physiology,Electrophysiology,Evoked Potentials,Glutamic Acid,Glutamic Acid: pharmacology,N-Methylaspartate,N-Methylaspartate: physiology,Pyramidal Cells,Pyramidal Cells: physiology,Rats,Receptors, N-Methyl-D-Aspartate,Receptors, N-Methyl-D-Aspartate: antagonists \& inh,Receptors, N-Methyl-D-Aspartate: physiology,Synapses,Synapses: physiology,Tetrodotoxin,Tetrodotoxin: pharmacology},
month = mar,
number = {6775},
pages = {285--9},
pmid = {10749211},
shorttitle = {Nature},
title = {{NMDA spikes in basal dendrites of cortical pyramidal neurons.}},
